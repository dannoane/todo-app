type Query {
    todoById(id: ID): Todo
    allTodos: [Todo]
}

type Mutation {
    createTodo(text: String): Todo
    setTodoStatus(id: ID, completed: Boolean): Todo
    deleteTodo(id: ID): Todo
    updateTodoText(id: ID, text: String): Todo
}

type Todo {
    id: ID
    text: String
    completed: Boolean
}